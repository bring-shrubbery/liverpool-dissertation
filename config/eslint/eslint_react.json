{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [1, 120, 2, {
            "ignoreComments": true
        }],
        "no-console": [
            "error", 
            { 
                "allow": [
                    "warn", 
                    "error"
                ] 
            }
        ]
    },
    "extends": [
        "eslint:recommended", 
        "plugin:react/recommended"
    ]
}